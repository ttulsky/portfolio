{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nimport * as emailjs from \"@emailjs/browser\";\nimport BtmHero from \"../components/BtmHero.vue\";\nexport default {\n  name: \"ContactUs\",\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      message: \"\"\n    };\n  },\n  components: {\n    BtmHero: BtmHero\n  },\n  methods: {\n    sendEmail(e) {\n      try {\n        emailjs.sendForm(process.env.VUE_APP_EMAILJS_SERVICE_ID, process.env.VUE_APP_EMAILJS_TEMPLATE_ID, e.target, process.env.VUE_APP_EMAILJS_USER_ID, {\n          name: this.name,\n          email: this.email,\n          message: this.message\n        });\n      } catch (err) {\n        if (err instanceof ReferenceError) {\n          alert(\"JSON Error: \" + err.message);\n        } else {\n          throw err; // rethrow\n        }\n      }\n      // Reset form field\n      this.name = \"\";\n      this.email = \"\";\n      this.message = \"\";\n    }\n  }\n};","map":{"version":3,"names":["emailjs","BtmHero","name","data","email","message","components","methods","sendEmail","e","sendForm","process","env","VUE_APP_EMAILJS_SERVICE_ID","VUE_APP_EMAILJS_TEMPLATE_ID","target","VUE_APP_EMAILJS_USER_ID","err","ReferenceError","alert"],"sources":["/Users/tylertulsky/Tylers Portfolio/portfolio/src/views/Contact.vue"],"sourcesContent":["<template>\n  <BtmHero />\n\n  <v-card class=\"top\">\n    <div class=\"container\">\n      <form @submit.prevent=\"sendEmail\">\n        <label>Name</label>\n        <input type=\"text\" v-model=\"name\" name=\"name\" placeholder=\"Your Name\" />\n        <label>Email</label>\n        <input\n          type=\"email\"\n          v-model=\"email\"\n          name=\"email\"\n          placeholder=\"Your Email\"\n        />\n        <label>Message</label>\n        <textarea\n          name=\"message\"\n          v-model=\"message\"\n          cols=\"30\"\n          rows=\"5\"\n          placeholder=\"Please let me know if you have any qestions or a specific project in mind!\"\n        >\n        </textarea>\n\n        <input type=\"submit\" value=\"Send\" />\n      </form>\n    </div>\n    <div class=\"contacts\">\n      <v-btn\n        href=\"https://www.linkedin.com/in/tyler-tulsky-a59103141/\"\n        target=\"https://www.linkedin.com/in/tyler-tulsky-a59103141/\"\n      >\n        linkedIN\n      </v-btn>\n      <v-btn\n        href=\"https://github.com/ttulsky\"\n        target=\"https://github.com/ttulsky\"\n      >\n        GitHub\n      </v-btn>\n    </div>\n    <h3 class=\"email\">Reach out to me directly at ttulsky@gmail.com</h3>\n  </v-card>\n</template>\n\n<script>\nimport * as emailjs from \"@emailjs/browser\";\nimport BtmHero from \"../components/BtmHero.vue\";\n\nexport default {\n  name: \"ContactUs\",\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      message: \"\",\n    };\n  },\n  components: {\n    BtmHero: BtmHero,\n  },\n  methods: {\n    sendEmail(e) {\n      try {\n        emailjs.sendForm(\n          process.env.VUE_APP_EMAILJS_SERVICE_ID,\n          process.env.VUE_APP_EMAILJS_TEMPLATE_ID,\n          e.target,\n          process.env.VUE_APP_EMAILJS_USER_ID,\n          {\n            name: this.name,\n            email: this.email,\n            message: this.message,\n          }\n        );\n      } catch (err) {\n        if (err instanceof ReferenceError) {\n          alert(\"JSON Error: \" + err.message);\n        } else {\n          throw err; // rethrow\n        }\n      }\n      // Reset form field\n      this.name = \"\";\n      this.email = \"\";\n      this.message = \"\";\n    },\n  },\n};\n</script>\n\n<style scoped>\n* {\n  box-sizing: border-box;\n}\n.email {\n  text-align: center;\n}\n\n.contacts {\n  padding-top: 25px;\n  text-align: center;\n  justify-content: space-around;\n}\n\n.contacts .v-btn {\n  margin: 0 10px;\n  background-color: #4675a6; /* Or any color you like */\n}\n\n.contacts .v-btn:hover {\n  background-color: #a8cff5; /* Or any color you like */\n}\n\n.top {\n  padding-top: 25px;\n  overflow-y: auto;\n  height: 100vh;\n}\n\nlabel {\n  float: left;\n}\n\ninput[type=\"text\"],\n[type=\"email\"],\ntextarea {\n  width: 100%;\n  padding: 12px;\n  border: 1px solid #b4b1b1;\n  border-radius: 4px;\n  box-sizing: border-box;\n  margin-top: 6px;\n  margin-bottom: 16px;\n  resize: vertical;\n}\n\ninput[type=\"submit\"] {\n  background-color: #4675a6;\n  color: rgb(1, 1, 1);\n  padding: 12px 20px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\ninput[type=\"submit\"]:hover {\n  background-color: #a2cbf5;\n}\n\n.container {\n  display: block;\n  margin: auto;\n  text-align: center;\n  border-radius: 5px;\n  background-color: #d2e8f4;\n  padding: 20px;\n  width: 50%;\n}\n</style>\n"],"mappings":";AA+CA,OAAO,KAAKA,OAAM,MAAO,kBAAkB;AAC3C,OAAOC,OAAM,MAAO,2BAA2B;AAE/C,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVL,OAAO,EAAEA;EACX,CAAC;EACDM,OAAO,EAAE;IACPC,SAASA,CAACC,CAAC,EAAE;MACX,IAAI;QACFT,OAAO,CAACU,QAAQ,CACdC,OAAO,CAACC,GAAG,CAACC,0BAA0B,EACtCF,OAAO,CAACC,GAAG,CAACE,2BAA2B,EACvCL,CAAC,CAACM,MAAM,EACRJ,OAAO,CAACC,GAAG,CAACI,uBAAuB,EACnC;UACEd,IAAI,EAAE,IAAI,CAACA,IAAI;UACfE,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,OAAO,EAAE,IAAI,CAACA;QAChB,CACF,CAAC;MACH,EAAE,OAAOY,GAAG,EAAE;QACZ,IAAIA,GAAE,YAAaC,cAAc,EAAE;UACjCC,KAAK,CAAC,cAAa,GAAIF,GAAG,CAACZ,OAAO,CAAC;QACrC,OAAO;UACL,MAAMY,GAAG,EAAE;QACb;MACF;MACA;MACA,IAAI,CAACf,IAAG,GAAI,EAAE;MACd,IAAI,CAACE,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,OAAM,GAAI,EAAE;IACnB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}